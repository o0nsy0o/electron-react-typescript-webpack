{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "antd": ["components/index.tsx"],
      "antd/es/*": ["components/*"]
    },
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "esnext",
    "lib": ["dom", "es2018"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "lib", "es"]
}